[Argument]
chxm1023_enable = switch,false,false,tag=chxm1023
ddgksf2013_enable = switch,false,false,tag= ddgksf2013
Guding88_enable = switch,false,false,tag= Guding88

# 启用开关
enable-switch = switch,false,tag=启用解锁,desc=是否启用iTunes验证解锁


[Script]
# chxm1023
http-response pattern=^https?:\/\/buy\.itunes\.apple\.com\/verifyReceipt$ script-path=https://raw.githubusercontent.com/chxm1023/Rewrite/main/iTunes.js, requires-body=false, tag=解锁会员@chxm1023, enabled=false,enable={chxm1023_enable}
# ddgksf2013
http-response pattern=^https?:\/\/buy\.itunes\.apple\.com\/verifyReceipt$ script-path=https://raw.githubusercontent.com/SXIE-ai/Loon/refs/heads/main/plugins/iTunes/iTunes2.js, requires-body=false, tag=解锁会员@ddgksf2013, enabled=false,enable={ddgksf2013_enable}
# Guding88
http-response pattern=^https?:\/\/buy\.itunes\.apple\.com\/verifyReceipt$ script-path=https://raw.githubusercontent.com/Guding88/Script/main/APPheji_iTunes.js, requires-body=false, tag=解锁会员@Guding88, enabled=false,enable={Guding88_enable}

[MITM]
hostname = buy.itunes.apple.com
